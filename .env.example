# FastAPI app settings (.env example)

# Security
SECRET_KEY=dev-secret-change-me
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60

# Database (async SQLite by default)
DATABASE_URL=sqlite+aiosqlite:///./data.db

# OCR backend (OpenAI-compatible)
LLM_BASE_URL=http://localhost:8000/v1
LLM_API_KEY=token-abc123
LLM_MODEL=deepseek-ai/DeepSeek-OCR
LLM_PROMPT=Free OCR, output markdown.

# Demo bootstrap user
BOOTSTRAP_USER=demo
BOOTSTRAP_PASS=demo123

# Auth toggle
AUTH_ENABLED=true
ANON_USERNAME=anonymous

